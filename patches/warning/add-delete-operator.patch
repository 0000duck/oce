+  {
+
+  }
 
   //! Empty constructor <br>
   Standard_EXPORT   BinLDrivers_DocumentSection();
Index: oce/inc/Interface_FileParameter.hxx
===================================================================
--- oce.orig/inc/Interface_FileParameter.hxx
+++ oce/inc/Interface_FileParameter.hxx
@@ -52,6 +52,10 @@ public:
   {
     if (anAddress) Standard::Free((Standard_Address&)anAddress); 
   }
+  void operator delete (void *,void *)
+  {
+
+  }
 
   
   Standard_EXPORT   Interface_FileParameter();
Index: oce/inc/gp_XY.hxx
===================================================================
--- oce.orig/inc/gp_XY.hxx
+++ oce/inc/gp_XY.hxx
@@ -57,6 +57,10 @@ public:
   {
