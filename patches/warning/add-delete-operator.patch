===================================================================
--- oce.orig/inc/gp_XYZ.hxx
+++ oce/inc/gp_XYZ.hxx
@@ -57,6 +57,10 @@ public:
   {
     if (anAddress) Standard::Free((Standard_Address&)anAddress); 
   }
