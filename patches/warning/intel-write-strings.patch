From 0c410ab55c5412a5c99ba095a2c693f119bfb9e4 Mon Sep 17 00:00:00 2001
From: Jake <thatcadguy@users.noreply.github.com>
Date: Wed, 18 Sep 2013 21:49:09 -0700
Subject: [PATCH] Change MB_DESC to use const _TCHAR*

Fixes -Wwrite-strings warning about implicit conversion from string literal to _TCHAR*.
---
 src/OSD/OSD_WNT_1.hxx | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

Index: oce/src/OSD/OSD_File.cxx
===================================================================
--- oce.orig/src/OSD/OSD_File.cxx
+++ oce/src/OSD/OSD_File.cxx
@@ -854,7 +854,7 @@ Standard_Boolean OSD_File::IsExecutable(
 
 #if defined(__CYGWIN32__) || defined(__MINGW32__)
 #define VAC
-#define _int64 int
+#define _int64 int64_t
 #endif
 
 #pragma comment( lib, "WSOCK32.LIB"  )
@@ -1685,7 +1685,7 @@ OSD_File OSD_File :: BuildTemporary () {
 typedef struct _osd_wnt_key {
 
                 HKEY   hKey;
-                LPTSTR keyPath;
+                LPCTSTR keyPath;
 
                } OSD_WNT_KEY;
 
Index: oce/src/OSD/OSD_WNT_1.hxx
===================================================================
--- oce.orig/src/OSD/OSD_WNT_1.hxx
+++ oce/src/OSD/OSD_WNT_1.hxx
@@ -83,7 +83,7 @@ typedef struct _MB_DESC {
 
                 MB_ITEMTYPE   itemType;
                 _TINT         itemId;
-                _TCHAR*       buttonLabel;
+                const _TCHAR* buttonLabel;
 
                } MB_DESC, *LPMB_DESC;
 
