Subject: [PATCH] [warning-fix][initialization-order]

---
 src/OpenGl/OpenGl_PrinterContext.cxx             |    6 ++++--
 src/OpenGl/OpenGl_Workspace.cxx                  |    9 +++++----
 src/QANewModTopOpe/QANewModTopOpe_Glue.cxx       |    4 ++--
 src/QANewModTopOpe/QANewModTopOpe_Limitation.cxx |    6 +++---
 src/ViewerTest/ViewerTest_ViewerCommands.cxx     |   11 ++++++-----
 5 files changed, 20 insertions(+), 16 deletions(-)

Index: oce/src/ViewerTest/ViewerTest_ViewerCommands.cxx
===================================================================
--- oce.orig/src/ViewerTest/ViewerTest_ViewerCommands.cxx
+++ oce/src/ViewerTest/ViewerTest_ViewerCommands.cxx
@@ -3559,13 +3559,13 @@ V3d_TextItem::V3d_TextItem (const TColle
                             const Aspect_TypeOfDisplayText& theTypeOfDisplay,
                             const Handle(Visual3d_Layer)& theLayer)
  : myX1 (theX1), myY1 (theY1),
-   myText (theText),
    myHeight (theHeight),
-   myLayer (theLayer),
+   myText (theText),
+   myFontName (theFontName),
    myColor (theColor),
    mySubtitleColor (theSubtitleColor),
    myType (theTypeOfDisplay),
-   myFontName (theFontName)
+   myLayer (theLayer)
 {
   if (!myLayer.IsNull ())
     myLayer->AddLayerItem (this);
@@ -3623,8 +3623,9 @@ V3d_LineItem::V3d_LineItem(Standard_Real
                            Aspect_TypeOfLine theType,
                            Standard_Real theWidth,
                            Standard_Real theTransp) :
-  myX1(X1), myY1(Y1), myX2(X2), myY2(Y2), myLayerMgr(theLayerMgr),
-  myType(theType), myWidth(theWidth), myTransparency(theTransp)
+  myX1(X1), myY1(Y1), myX2(X2), myY2(Y2),
+  myWidth(theWidth), myTransparency(theTransp),
+  myType(theType), myLayerMgr(theLayerMgr)
 {
   if (myLayerMgr && !myLayerMgr->Overlay().IsNull())
     myLayerMgr->Overlay()->AddLayerItem (this);
