 7 files changed, 16 insertions(+)

Index: oce/src/BRepAlgo/BRepAlgo.cxx
===================================================================
--- oce.orig/src/BRepAlgo/BRepAlgo.cxx
+++ oce/src/BRepAlgo/BRepAlgo.cxx
@@ -464,6 +464,8 @@ TopoDS_Edge  BRepAlgo::ConcatenateWireC0
 		    }
 		  break;
 		}
+        default:
+          break;
 	      } //end of switch and else
 	  if (Done)
 	    {
Index: oce/src/BRepFill/BRepFill_Pipe.cxx
===================================================================
--- oce.orig/src/BRepFill/BRepFill_Pipe.cxx
+++ oce/src/BRepFill/BRepFill_Pipe.cxx
@@ -142,6 +142,8 @@ void BRepFill_Pipe::Perform(const TopoDS
   case GeomFill_IsDiscreteTrihedron:
     TLaw = new GeomFill_DiscreteTrihedron();
     break;
+  default:
+    break;
   }
   Handle(GeomFill_CurveAndTrihedron) Loc = 
     new (GeomFill_CurveAndTrihedron) (TLaw);
Index: oce/src/Contap/Contap_HCurve2dToolGen.gxx
===================================================================
--- oce.orig/src/Contap/Contap_HCurve2dToolGen.gxx
+++ oce/src/Contap/Contap_HCurve2dToolGen.gxx
@@ -48,6 +48,8 @@ Standard_Integer Contap_HCurve2dToolGen:
       nbs/= U1-U0;
       if(nbs < 2.0) nbs = 2.;
       break;
+    default:
+      break;
   }
   if (nbs>50.)
     nbs = 50.;
Index: oce/src/HLRBRep/HLRBRep_Curve.cxx
===================================================================
--- oce.orig/src/HLRBRep/HLRBRep_Curve.cxx
+++ oce/src/HLRBRep/HLRBRep_Curve.cxx
@@ -77,6 +77,8 @@ HLRBRep_Curve::Parameter2d (const Standa
 
     case GeomAbs_Ellipse:
       return P3d + myOX;
+    default:
+      break;
   }
   return P3d;
 }
Index: oce/src/OpenGl/OpenGl_PrimitiveArray.cxx
===================================================================
--- oce.orig/src/OpenGl/OpenGl_PrimitiveArray.cxx
+++ oce/src/OpenGl/OpenGl_PrimitiveArray.cxx
