     case TelTriangleFansArrayType:
       glColor3fv (theInteriorColour->rgb);
       break;
+    default:
+      break;
   }
 
   // Temporarily disable environment mapping
@@ -553,6 +555,8 @@ OpenGl_PrimitiveArray::OpenGl_PrimitiveA
     case TelTriangleFansArrayType:
       myDrawMode = GL_TRIANGLE_FAN;
       break;
+    default:
+      break;
   }
 }
 
Index: oce/src/TNaming/TNaming_DeltaOnModification.cxx
===================================================================
